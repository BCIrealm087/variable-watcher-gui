{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/vite-src/dist/",
  "url": "/",
  "port": 3000,
  "enableServer": true,
  "enableExtensions": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "storage.*",
    "os.showOpenDialog",
    "filesystem.readDirectory",
    "filesystem.readFile",
    "extensions.getStats",
    "window.*"
  ],
  "extensions": [
    {
      "id": "serial-input-python",
      "path": "python3",
      "arguments": ["--py", "/extensions/serial-input-python/main.py"]
    }
  ],
  "singlePageServe": true,
  "modes": {
    "window": {
      "title": "myapp",
      "width": 1280,
      "height": 800,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/vite-src/public/neutralino.png",
      "enableInspector": true, 
      "useSavedState": true
    }
  },
  "cli": {
    "binaryName": "variable-watcher-gui",
    "resourcesPath": "/vite-src/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "6.2.0",
    "clientVersion": "6.2.0",
    "frontendLibrary": {
      "patchFile": "/vite-src/index.html",
      "devUrl": "http://localhost:5173",
      "projectPath": "/vite-src/",
      "initCommand": "npm install",
      "devCommand": "npm run dev",
      "buildCommand": "npm run build"
    }
  }
}